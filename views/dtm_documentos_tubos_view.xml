<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="dtm_documentos_tubos_view_tree" model="ir.ui.view">
            <field name="name">dtm_documentos_tubos_tree</field>
            <field name="model">dtm.documentos.tubos</field>
            <field name="arch" type="xml">
                <tree  >
                    <field name="nombre"/>
                    <field name="contador" />
                </tree>
            </field>
        </record>

        <record id="dtm_documentos_tubos_view_form" model="ir.ui.view">
        <field name="name">dtm_documentos_tubos_view_form</field>
        <field name="model">dtm.documentos.tubos</field>
        <field name="arch" type="xml">
            <form >
                <field name="documentos" filename="nombre" widget="pdf_viewer"></field>
                <field name="nombre" invisible="1"></field>
            </form>
        </field>
    </record>
    </data>



</odoo>
